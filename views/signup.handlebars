<main role="main" class="container">
  <div class="jumbotron bg-info">
    <h1 class="text-light">SignUp</h1>
    <p class="lead text-light">Let's get you started</p>
    <br>
    <br>
    <!-- row -->
    <div class="row">
      <form>
        <div class="form-group">
          <label for="Name">Name</label>
          <input type="Name" class="form-control" id="Name" aria-describedby="NameHelp" placeholder="Enter Name">
        </div>
        <div class="form-group">
          <label for="Number">Number</label>
          <input type="password" class="form-control" id="Number" placeholder="Password">
        </div>
        <div class="form-check">
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
      </form>
    </div>
  </div>
</main>
