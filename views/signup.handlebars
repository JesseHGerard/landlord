<main role="main" class="container">
  <div class="jumbotron bg-info">
    <h1 class="text-light">SignUp</h1>
    <p class="lead text-light">Let's get you started</p>
    <br>
    <br>
    <!-- row -->
    <!-- Name -->
    <!-- Email -->
    <!-- Password -->
    <!-- Building -->
    <!-- Apartment -->
    <!-- Phone Number -->

    <div class="row">
      <form>
        <div class="form-group">
          <label for="Name">Name</label>
          <input class="form-control" id="name" aria-describedby="NameHelp" placeholder="Enter Name">
        </div>
        <div class="form-group">
          <label for="Email">Email</label>
          <input class="form-control" id="email" placeholder="Email">
        </div>
        <div class="form-group">
          <label for="Password">Password</label>
          <input type="password" class="form-control" id="password" placeholder="Password">
        </div>
        <div class="form-group">
          <label for="Building">Building</label>
          <input class="form-control" id="building" placeholder="Building">
        </div>
        <div class="form-group">
          <label for="Apartment">Apartment</label>
          <input class="form-control" id="apartment" placeholder="Apartment">
        </div>
        <div class="form-group">
          <label for="Number">Number</label>
          <input class="form-control" id="number" placeholder="Number">
        </div>
        <div class="form-check">
        </div>
        <button type="submit" class="btn btn-primary" id="newSign">Submit</button>
        </form>
    </div>
  </div>
</main>
